{
  "name": "pptr-mocha",
  "version": "1.0.0",
  "description": "Test framework with mocha steps and builder class pattern",
  "keywords": [
    "mocha",
    "puppeteer framework"
  ],
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout=30000 /build/tests/**.js",
    "clean": "rm -rf build",
    "build": "babel --preset es2015 -d build/ src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzmitry-duboyski/pptr-mocha.git"
  },
  "author": "",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/dzmitry-duboyski/pptr-mocha/issues"
  },
  "homepage": "https://github.com/dzmitry-duboyski/pptr-mocha#readme",
  "dependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mocha-steps": "^1.3.0",
    "puppeteer": "^19.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
